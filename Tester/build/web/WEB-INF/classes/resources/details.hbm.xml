<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojodriver.StudentDetail" schema="tester" table="user_detail">
        
    <id name="email"/>    
    <property name="age"></property>
    <property name="college"></property>
    <property name="qualification"></property>
    <property name="language"></property>
    <property name="technical"></property>
    </class>
</hibernate-mapping>
        